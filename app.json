{
  "expo": {
    "name": "Grammachat",
    "slug": "grammachat",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./frontend/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./frontend/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grammachat.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./frontend/assets/adaptive-icon.png",
        "backgroundColor": "#6366f1"
      },
      "package": "com.grammachat.app"
    },
    "web": {
      "favicon": "./frontend/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-notifications",
      "expo-asset",
      "expo-font",
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "notification": {
      "icon": "./frontend/assets/notification-icon.png",
      "color": "#6366f1"
    },
    "extra": {
      "eas": {
        "projectId": "0ff4ca6f-09d3-4931-b376-b14d33f855dc"
      }
    },
    "owner": "gauvin"
  }
}

